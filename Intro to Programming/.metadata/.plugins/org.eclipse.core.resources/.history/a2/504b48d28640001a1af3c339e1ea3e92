package dontKnowWhatThisIsFor;

public class toGETher {
	
	private String name; 
	private int age;

	public static void main(String[] args) {
		
		toGETher account= new toGETher();				//this is how to make an object! creating object account
		
		account.setName("Nadine");
		account.setAge(123);
		
		System.out.println(account.getName());
		System.out.println(account.getAge());

		account.printDetails();								//not using static because only accessing from the class. hence we use 
															// account which is a method from the class??
	}
	
	/*public void setName(String name) {
