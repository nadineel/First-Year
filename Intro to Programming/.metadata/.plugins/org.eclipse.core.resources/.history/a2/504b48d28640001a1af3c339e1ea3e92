																				//right click source getter and setters!!!!!!!!!
	
	public void printDetails() {
		System.out.println(name +" ,"+age);
	}

	public String getName() {
		return name;
	}

	public void setName(String name) {
		this.name = name;
	}

	public int getAge() {
		return age;
	}

	public void setAge(int age) {
		this.age = age;
	}

}
